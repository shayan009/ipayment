<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Rotate3dAnimation">
        <attr name="rollType" format="enum">
            <enum name="x" value="0" />
            <enum name="y" value="1" />
            <enum name="z" value="2" />
        </attr>
        <attr name="fromDeg" format="float" />
        <attr name="toDeg" format="float" />
        <attr name="pivotX1" format="fraction" />
        <attr name="pivotY1" format="fraction" />
    </declare-styleable>
    <declare-styleable name="SelectableRoundedImageView"></declare-styleable>

    <declare-styleable name="ToolDotProgress">
        <attr name="color" format="color" />
        <attr name="count" format="integer" />
        <attr name="timeout" format="integer" />
    </declare-styleable>

</resources>